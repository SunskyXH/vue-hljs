{
  "name": "vue-hljs",
  "version": "2.0.0",
  "description": "Highlight.js plugin for Vue.js 2.0",
  "main": "./dist/vue-hljs.umd.js",
  "module": "./dist/vue-hljs.es.js",
  "exports": {
    ".": {
      "import": "./dist/my-lib.es.js",
      "require": "./dist/my-lib.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "nyc ava",
    "clean": "rm -rf dist/"
  },
  "ava": {
    "files": [
      "test/test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sunskyxh/vue-hljs.git"
  },
  "keywords": [
    "highlight",
    "hljs",
    "vue"
  ],
  "author": {
    "name": "SunskyXH",
    "email": "SunskyXH@gmail.com",
    "url": "https://sunskyxh.github.io/"
  },
  "maintainers": [
    {
      "name": "SunskyXH",
      "email": "SunskyXH@gmail.com",
      "url": "https://sunskyxh.github.io/"
    }
  ],
  "license": "BSD-3",
  "devDependencies": {
    "ava": "^0.25.0",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "highlight.js": "^10.5.0",
    "nyc": "^13.1.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.50",
    "vue": "^2.6.12"
  },
  "peerDependencies": {
    "highlight.js": "^10.0.0",
    "vue": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/sunskyxh/vue-hljs/issues"
  },
  "homepage": "https://github.com/sunskyxh/vue-hljs#readme",
