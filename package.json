      "url": "https://sunskyxh.me/"
    }
  ],
  "license": "BSD-3",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.8",
    "@vitest/coverage-v8": "3.2.1",
    "highlight.js": "^11.11.1",
    "jsdom": "^26.1.0",
    "typescript": "^5.8.3",
    "unplugin-dts": "1.0.0-beta.0",
    "vite": "^6.3.5",
